{
  "$schema": "../../schema/templateSchema.json",
  "epiHiperSchema": "https://github.com/NSSAC/EpiHiper-Schema/blob/master/schema/templateSchema.json",
  "ann:id": "PercentOfInfectedPopulation",
  "ann:label": "Initial infections - percentage",
  "ann:description": "Initially infect the specified percentage of the population.",
  "template": {
    "epiHiperSchema": "https://github.com/NSSAC/EpiHiper-Schema/blob/master/schema/initializationSchema.json",
    "initializations": [
      {
        "target": {
          "elementType": "node"
        },
        "sampling": {
          "type": "group",
          "number": 2,
          "sampled": {
            "foreach": [
              {
                "operations": [
                  {
                    "target": {
                      "node": {
                        "property": "healthState"
                      }
                    },
                    "operator": "=",
                    "value": {
                      "healthState": "Isymp"
                    }
                  }
                ]
              }
            ]
          }
        }
      }
    ]
  },
  "userInput": [
    {
      "ann:id": "PercentOfInfectedPopulation",
      "ann:label": "Precent of population to initially infect",
      "ann:description": "Percentage of population that will be infected at the start of the simulation.",
      "value": [
        "/template/initializations/1/sampling/number"
      ],
      "exceads": {
        "minValue": 0,
        "maxValue": 1,
        "sweep": false,
        "calibrate": false
      }
    }
  ],
  "requirements": [
    {
      "value": {
        "healthState": "Isymp"
      }
    }
  ],
  "exceads": {
    "requires": {
      "allOf": [
        "diseaseModelTemplate1.json"
      ]
    }
  }
}
