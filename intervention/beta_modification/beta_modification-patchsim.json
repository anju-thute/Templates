{
  "$schema": "https://raw.githubusercontent.com/NSSAC/EpiHiper-Schema/master/schema/templateSchema.json",
  "epiHiperSchema": "https://raw.githubusercontent.com/NSSAC/EpiHiper-Schema/master/schema/templateSchema.json",
  "ann:id": "SeedingNumberPatchsim",
  "ann:label": "Seeding (number)",
  "ann:description": "Modify the beta schedule.",
  "template": {
    "$schema": "https://raw.githubusercontent.com/NSSAC/EpiHiper-Schema/master/schema/patchSimSchema.json",
    "epiHiperSchema": "https://raw.githubusercontent.com/NSSAC/EpiHiper-Schema/master/schema/patchSimSchema.json",
    "sets": [
      {
        "id": "custom_population",
        "ann:label": "Custom Sub Population",
        "scope": "local",
        "content": {
          "scope": "local",
          "elementType": "node"
        }
      }
    ],
    "interventions": [
      {
        "target": {
          "set": {
            "idRef": "custom_population"
          }
        },
        "foreach": [
          {
            "condition": {
              "and": [
                {
                  "left": {
                    "observable": "time"
                  },
                  "operator": ">=",
                  "right": 0
                },
                {
                  "left": {
                    "observable": "time"
                  },
                  "operator": "<",
                  "right": 180
                }
              ]
            },
            "operations": [
              {
                "target": {
                  "node": {
                    "property": "beta"
                  }
                },
                "operator": "*",
                "value": 0.33
              }
            ]
          }
        ]
      }
    ]
  },
  "userInput": [
    {
      "ann:id": "SubPopulation",
      "ann:label": "Sub Population",
      "ann:description": "A subset of the patches to which beta modification is applied",
      "value": [
        "/template/sets/1/content"
      ],
      "exceads": {
        "sweep": false,
        "calibrate": false,
        "attributeType": "text",
        "isApplicable": false
      }
    },
    {
      "ann:id": "StartTime",
      "ann:label": "Start time",
      "ann:description": "Time when the modification starts",
      "value": [
        "/template/interventions/1/foreach/1/condition/and/1/right"
      ],
      "exceads": {
        "minValue": 0,
        "maxValue": 999,
        "sweep": true,
        "calibrate": false
      }
    },
    {
      "ann:id": "EndTime",
      "ann:label": "End time",
      "ann:description": "Time when the modification stops",
      "value": [
        "/template/interventions/1/foreach/1/condition/and/2/right"
      ],
      "exceads": {
        "minValue": 0,
        "maxValue": 999,
        "sweep": true,
        "calibrate": false
      }
    },
    {
      "ann:id": "Change",
      "ann:label": "Transmissibility change",
      "ann:description": "Relative transmissibility change",
      "value": [
        "/template/interventions/1/foreach/1/operations/1/value"
      ],
      "exceads": {
        "minValue": 0,
        "maxValue": 999,
        "sweep": true,
        "calibrate": false
      }
    }
  ],
  "requirements": [],
  "engines": [
    "PatchSim"
  ],
  "exceads": {}
}