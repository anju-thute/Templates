{
  "$schema": "../../../EpiHiper-Schema/schema/templateSchema.json",
  "epiHiperSchema": "https://github.com/NSSAC/EpiHiper-Schema/blob/master/schema/templateSchema.json",
  "ann:id": "Prophylax",
  "ann:label": "Prophylax Treatment of Household Members",
  "ann:description": "For identified cases provide prophylactic treatment to their household members",
  "template": {
    "epiHiperSchema": "https://github.com/NSSAC/EpiHiper-Schema/blob/master/schema/interventionSchema.json",
    "sets": [],
    "variables": [],
    "triggers": [],
    "interventions": [
      {
        "$comment": "Prophylax",
        "id": "interventionTemplate3_1",
        "target": {
          "elementType": "node",
          "scope": "global",
          "left": {
            "table": "montgomery_county_va",
            "field": "hid"
          },
          "operator": "in",
          "right": {
            "elementType": "dbField",
            "table": "montgomery_county_va",
            "field": "hid",
            "selector": {
              "set": {
                "idRef": "triggerTemplate4_prophylaxTrue"
              }
            }
          }
        },
        "foreach": [
          {
            "condition": {
              "and": [
                {
                  "left": {
                    "node": {
                      "property": "healthState"
                    }
                  },
                  "operator": "in",
                  "right": {
                    "valueList": {
                      "healthState": [
                        "S",
                        "E",
                        "I"
                      ]
                    }
                  }
                },
                {
                  "left": {
                    "node": {
                      "property": "nodeTrait",
                      "feature": "prophylax"
                    }
                  },
                  "operator": "==",
                  "right": {
                    "value": {
                      "trait": "nodeTrait",
                      "feature": "prophylax",
                      "enum": "false"
                    }
                  }
                }
              ]
            },
            "operations": [
              {
                "target": {
                  "node": {
                    "property": "susceptibilityFactor"
                  }
                },
                "operator": "*=",
                "value": {
                  "number": 0.4
                }
              },
              {
                "target": {
                  "node": {
                    "property": "nodeTrait",
                    "feature": "prophylax"
                  }
                },
                "operator": "=",
                "value": {
                  "trait": "nodeTrait",
                  "feature": "prophylax",
                  "enum": "true"
                }
              }
            ]
          },
          {
            "delay": 5,
            "condition": {
              "left": {
                "node": {
                  "property": "nodeTrait",
                  "feature": "prophylax"
                }
              },
              "operator": "==",
              "right": {
                "value": {
                  "trait": "nodeTrait",
                  "feature": "prophylax",
                  "enum": "true"
                }
              }
            },
            "operations": [
              {
                "target": {
                  "node": {
                    "property": "susceptibilityFactor"
                  }
                },
                "operator": "/=",
                "value": {
                  "number": 0.4
                }
              },
              {
                "target": {
                  "node": {
                    "property": "nodeTrait",
                    "feature": "prophylax"
                  }
                },
                "operator": "=",
                "value": {
                  "trait": "nodeTrait",
                  "feature": "prophylax",
                  "enum": "false"
                }
              }
            ]
          }
        ]
      }
    ]
  },
  "userInput": [
    {
      "ann:id": "DaysTreatment",
      "ann:label": "Days of Treatment",
      "ann:description": "Number days for the prophylactic treatment",
      "value": [
        "/template/interventions/1/foreach/2/delay"
      ],
      "exceads": {
        "minValue": 0,
        "maxValue": 999,
        "sweep": false,
        "calibrate": false
      }
    },
    {
      "ann:id": "ProphylaxEffectiveness",
      "ann:label": "Effectiveness of Treatment",
      "ann:description": "The effectiveness of the prophylactic treatment",
      "value": [
        "/template/interventions/1/foreach/1/operations/1/value/number",
        "/template/interventions/1/foreach/2/operations/1/value/number"
      ],
      "exceads": {
        "minValue": 0,
        "maxValue": 1,
        "sweep": false,
        "calibrate": false
      }
    }
  ],
  "requirements": [
    {
      "set": {
        "idRef": "triggerTemplate4_prophylaxTrue"
      }
    },
    {
      "property": "nodeTrait",
      "feature": "prophylax"
    },
    {
      "value": {
        "trait": "nodeTrait",
        "feature": "prophylax",
        "enum": "true"
      }
    },
    {
      "value": {
        "trait": "nodeTrait",
        "feature": "prophylax",
        "enum": "false"
      }
    },
    {
      "table": "montgomery_county_va",
      "field": "hid"
    },
    {
      "property": "nodeTrait",
      "feature": "prophylaxTrigger"
    },
    {
      "value": {
        "trait": "nodeTrait",
        "feature": "prophylaxTrigger",
        "enum": "true"
      }
    }
  ],
  "exceads": {
    "requires": {
      "allOf": [
        "treat_symptomatic_with_health_seeking_delay.json",
        "montgomery_county_va.json"
      ]
    },
    "interventionType": "PROPHYLAX"
  }
}
